{
  "name": "frontend-assessment",
  "version": "1.0.0",
  "description": "Frontend Assessment",
  "license": "MIT",
  "scripts": {
    "start": "npm run gulp --",
    "gulp": "gulp"
  },
  "author": "Project A Ventures",
  "repository": "https://github.com/project-a/frontend-assessment",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-run": "^3.2.0",
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "cheerio": "^0.20.0",
    "del": "^2.2.1",
    "faucet": "0.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "gulp-xo": "^0.12.0",
    "gutil": "^1.6.4",
    "jquery": "^3.1.0",
    "path": "^0.12.7",
    "require-dir": "^0.3.0",
    "tape": "^4.6.0",
    "utils-merge": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "xo": {
    "envs": [
      "browser",
      "jquery"
    ],
    "space": 4,
    "rules": {
      "one-var": [
        0,
        "never"
      ],
      "quote-props": [
        0,
        "consistent"
      ],
      "no-mixed-requires": 0,
      "space-before-function-paren": 0
    }
  }
}
